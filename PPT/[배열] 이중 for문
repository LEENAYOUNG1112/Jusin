#include <iostream>
using namespace std;

int main()
{
	const int SIZE = 5;
	int iArray[SIZE * SIZE] = { };
	int iCount(1);
	int i;
	int j;
	int iNum(0);
	
	for (i = 0; i < SIZE; i++)
	{
		for (j = 1; j <= SIZE; j++)
		{
			// iArray[시작점?]
			iArray[iNum] = iCount++; 
			cout << iArray[iNum] << "	";
			iNum++;
			
		}
		cout << endl;
	}
}
